splash_addr=0x43200000;
splash_file=boot/splash.raw;
load mmc ${bootpartition} ${splash_addr} ${splash_file};

gpio set ph27;
i2c dev 1;
i2c mw 0x3C 0x80 0xae; i2c mw 0x3C 0x80 0x81; i2c mw 0x3C 0x80 0x7f; i2c mw 0x3C 0x80 0xc0; i2c mw 0x3C 0x80 0xa8; i2c mw 0x3C 0x80 0x1f; i2c mw 0x3C 0x80 0xd3; i2c mw 0x3C 0x80 0x00; i2c mw 0x3C 0x80 0xd5; i2c mw 0x3C 0x80 0xa0; i2c mw 0x3C 0x80 0xd9; i2c mw 0x3C 0x80 0x22; i2c mw 0x3C 0x80 0xda; i2c mw 0x3C 0x80 0x12; i2c mw 0x3C 0x80 0xdb; i2c mw 0x3C 0x80 0x34;
i2c mw 0x3C 0x80 0x8d; i2c mw 0x3C 0x80 0x10; i2c mw 0x3C 0x80 0x20; i2c mw 0x3C 0x80 0x00; i2c mw 0x3C 0x80 0x21; i2c mw 0x3C 0x80 0x00; i2c mw 0x3C 0x80 0x7f; i2c mw 0x3C 0x80 0x22; i2c mw 0x3C 0x80 0x00; i2c mw 0x3C 0x80 0x03; i2c mw 0x3C 0x80 0xaf;
i2c write ${splash_addr} 0x3C 0x40 0x200 -s;

setenv bootargs console=ttyS0,115200 root=${bootdevice} rootwait panic=10 print-fatal-signals=1
ext4load mmc ${bootpartition} 0x49000000 /boot/${fdtfile}
ext4load mmc ${bootpartition} 0x46000000 /boot/zImage
env set fdt_high ffffffff
gpio set ph25
bootz 0x46000000 - 0x49000000
