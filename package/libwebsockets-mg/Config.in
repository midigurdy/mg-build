config BR2_PACKAGE_LIBWEBSOCKETS_MG
	bool "libwebsockets-mg"
	depends on BR2_USE_MMU # fork()
	select BR2_PACKAGE_ZLIB
	help
	  Libwebsockets is a lightweight pure C library built to use
	  minimal CPU and memory resources, and provide fast throughput
	  in both directions. This package is a copy of the original package for the MidiGurdy, without IPv6 support
      
	  http://libwebsockets.org/
